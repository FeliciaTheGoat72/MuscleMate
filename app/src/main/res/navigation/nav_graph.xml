        tools:layout="@layout/fragment_flies" />
    <fragment
        android:id="@+id/dipsFragment"
        android:name="com.example.musclemate.DipsFragment"
        android:label="fragment_dips"
        tools:layout="@layout/fragment_dips" />
    <fragment
        android:id="@+id/pushUpsFragment"
        android:name="com.example.musclemate.PushUpsFragment"
        android:label="fragment_push_ups"
        tools:layout="@layout/fragment_push_ups" />
    <fragment
        android:id="@+id/bendOverRowFragment"
        android:name="com.example.musclemate.BendOverRowFragment"
        android:label="fragment_bend_over_row"
        tools:layout="@layout/fragment_bend_over_row" />
    <fragment
        android:id="@+id/pullupFragment"
        android:name="com.example.musclemate.PullUpFragment"
        android:label="fragment_pullup"
        tools:layout="@layout/fragment_pull_up" />
    <fragment
        android:id="@+id/deadliftFragment"
        android:name="com.example.musclemate.DeadliftFragment"
        android:label="fragment_deadlift"
        tools:layout="@layout/fragment_deadlift" />
    <fragment
        android:id="@+id/uprightRowFragment"
        android:name="com.example.musclemate.UprightRowFragment"
        android:label="fragment_upright_row"
        tools:layout="@layout/fragment_upright_row" />
    <fragment
        android:id="@+id/dbRowFragment"
        android:name="com.example.musclemate.DbRowFragment"
        android:label="fragment_db_row"
        tools:layout="@layout/fragment_db_row" />
    <fragment
        android:id="@+id/RDLFragment"
        android:name="com.example.musclemate.RDLFragment"
        android:label="fragment_r_d_l"
        tools:layout="@layout/fragment_r_d_l" />
    <fragment
        android:id="@+id/splitSquatFragment"
        android:name="com.example.musclemate.SplitSquatFragment"
        android:label="fragment_split_squat"
        tools:layout="@layout/fragment_split_squat" />
    <fragment
        android:id="@+id/squatFragment"
        android:name="com.example.musclemate.SquatFragment"
        android:label="fragment_squat"
        tools:layout="@layout/fragment_squat" />
    <fragment
        android:id="@+id/lungeFragment"
        android:name="com.example.musclemate.LungeFragment"
        android:label="fragment_lunge"
        tools:layout="@layout/fragment_lunge" />
    <fragment
        android:id="@+id/bridgeFragment"
        android:name="com.example.musclemate.BridgeFragment"
        android:label="fragment_bridge"
        tools:layout="@layout/fragment_bridge" />
</navigation>